# Database Configuration (using SQLite for demo)
DATABASE_URL=sqlite:./authcenter.db
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=demo-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Google OAuth Configuration (demo values - need real ones)
GOOGLE_CLIENT_ID=demo-google-oauth-client-id
GOOGLE_CLIENT_SECRET=demo-google-oauth-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/api/auth/google/callback

# Application Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Logging
LOG_LEVEL=info